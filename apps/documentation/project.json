{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"sourceRoot": "apps/documentation/src",
	"prefix": "ng-doc",
	"targets": {
		"build": {
			"executor": "./dist/libs/builder:browser",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/apps/documentation",
				"index": "apps/documentation/src/index.html",
				"main": "apps/documentation/src/main.ts",
				"polyfills": "apps/documentation/src/polyfills.ts",
				"tsConfig": "apps/documentation/tsconfig.app.json",
				"inlineStyleLanguage": "scss",
				"assets": [
					"apps/documentation/src/favicon.ico",
					"apps/documentation/src/assets",
					{
						"glob": "**/*",
						"input": "dist/libs/ui-kit/assets",
						"output": "assets"
					},
					{
						"glob": "**/*",
						"input": "dist/libs/builder/generated/assets",
						"output": "assets"
					}
				],
				"styles": ["apps/documentation/src/styles.scss"],
				"scripts": [],
				"allowedCommonJsDependencies": ["prettier"]
			},
			"configurations": {
				"production": {
					"ngDoc": {
						"pages": "apps/documentation"
					},
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "500kb",
							"maximumError": "1mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "2kb",
							"maximumError": "4kb"
						}
					],
					"fileReplacements": [
						{
							"replace": "apps/documentation/src/environments/environment.ts",
							"with": "apps/documentation/src/environments/environment.prod.ts"
						}
					],
					"outputHashing": "all",
					"sourceMap": true,
					"optimization": false,
					"buildOptimizer": false,
					"aot": false
				},
				"development": {
					"ngDoc": {
						"pages": "apps/documentation"
					},
					"buildOptimizer": false,
					"optimization": false,
					"vendorChunk": true,
					"extractLicenses": false,
					"sourceMap": true,
					"namedChunks": true,
					"aot": false
				}
			},
			"defaultConfiguration": "production",
			"dependsOn": [
				{
					"projects": "dependencies",
					"target": "build"
				}
			]
		},
		"pre-serve": {
			"executor": "@nrwl/workspace:run-commands",
			"options": {
				"commands": [
					"symlink-dir dist/libs/core dist/libs/builder/node_modules/@ng-doc/core"
				]
			},
			"dependsOn": [
				{
					"projects": "dependencies",
					"target": "build"
				}
			]
		},
		"serve": {
			"executor": "./dist/libs/builder:dev-server",
			"configurations": {
				"production": {
					"browserTarget": "documentation:build:production",
					"ngDoc": {
						"pages": "apps/documentation",
						"tsConfig": "tsconfig.base.json"
					}
				},
				"development": {
					"browserTarget": "documentation:build:development",
					"ngDoc": {
						"pages": "apps/documentation",
						"tag": "develop"
					}
				}
			},
			"defaultConfiguration": "development",
			"dependsOn": [
				{
					"projects": "self",
					"target": "pre-serve"
				}
			]
		},
		"extract-i18n": {
			"executor": "@angular-devkit/build-angular:extract-i18n",
			"options": {
				"browserTarget": "documentation:build"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": ["apps/documentation/**/*.ts", "apps/documentation/**/*.html"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/apps/documentation"],
			"options": {
				"jestConfig": "apps/documentation/jest.config.ts",
				"passWithNoTests": true
			}
		}
	},
	"tags": ["type:app", "app:documentation"],
	"implicitDependencies": ["builder"]
}
