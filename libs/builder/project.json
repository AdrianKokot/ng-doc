{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/builder/src",
  "projectType": "library",
  "targets": {
    "pre-build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/builder",
        "main": "libs/builder/src/index.ts",
        "tsConfig": "libs/builder/tsconfig.lib.json",
        "assets": [
          "libs/builder/src/templates/**/*",
          "libs/builder/builders.json"
        ]
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "node build-schema.js --from=\"libs/builder/src/**/*(browser|dev-server)/schema.json\" --to=\"dist/libs/builder/src\""
        ]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs builder {args.ver} {args.tag}"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/builder/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/builder"
      ],
      "options": {
        "jestConfig": "libs/builder/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "core",
    "app"
  ]
}
