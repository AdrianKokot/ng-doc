// @ts-nocheck
import {Component, NgModule, Type, ChangeDetectionStrategy} from '@angular/core';
import {RouterModule} from '@angular/router';
import {CommonModule} from '@angular/common';
import {NgDocPageModule, NgDocRootPage, NgDocDemoAssets} from '@ng-doc/app';
import pageMd from '!!raw-loader!{{ page.builtPagePath }}';
{%- if page.pageDependenciesImport %}
// noinspection ES6UnusedImports
import pageDependencies from '{{ page.pageDependenciesImport }}';
{%- endif %}
{%- if page.componentsAssets %}
// noinspection ES6UnusedImports
import demoAssets from '{{ page.componentsAssets }}';
{%- endif %}

@Component({
	template: `<ng-doc-page></ng-doc-page>`,
	changeDetection: ChangeDetectionStrategy.OnPush,
	providers: [{provide: NgDocRootPage, useExisting: {{ page.componentName }}}],
})
export class {{ page.componentName }} extends NgDocRootPage {
	readonly pageMd: string = pageMd;
	{%- if page.pageDependenciesFile %}
	readonly demo: Record<string, Type<unknown>> | undefined = pageDependencies.demo;
	{%- else %}
	readonly demo: Record<string, Type<unknown>> | undefined = undefined;
	{%- endif %}
	{%- if page.componentsAssets %}
	readonly demoAssets: NgDocDemoAssets | undefined = demoAssets;
	{%- else %}
	readonly demoAssets: NgDocDemoAssets | undefined = undefined;
	{%- endif %}

	constructor() {
		super();
		console.log(this.pageMd, this.demo, this.demoAssets);
	}
}

@NgModule({
	declarations: [{{ page.componentName }}],
	imports: [
		CommonModule,
		NgDocPageModule,
		{%- if page.pageDependenciesFile %}
		pageDependencies.module,
		{%- endif %}
		RouterModule.forChild([{
			path: '',
			component: {{ page.componentName }},
		}]),
	],
	exports: [RouterModule]
})
export class {{ page.moduleName }} {
}

